{
  "expo": {
    "name": "qr-app",
    "slug": "qr-app",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/qr-logo.png",
    "scheme": "qrapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "6",
      "supportsTablet": true,
      "bundleIdentifier": "com.sameershaik.qrapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/qr-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/qr-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/qr-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6961fb3e-e2ac-453d-9639-e78b8c474ac3"
      }
    }
  }
}
